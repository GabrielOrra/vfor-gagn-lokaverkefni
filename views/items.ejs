<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= title %></title>
</head>
<body>
  <% for (let key in comics) {
    const comic = comics[key];
  %>
    <h3><%= comic.title %></h3>
    <img src="images/<%= comic.image %>" alt="">
    <p><%= comic.price %></p>
    <form action="/images" method="post">
      <input type="hidden" name="comic" value="<%= comic.id %>">
      <input type="button" value="Add to cart">  
    </form>

  }  %>
</body>
</html>